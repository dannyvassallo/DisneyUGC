<main>
  <div class="container">
    <div class="row">
      <div class="col s12">
        <%= link_to "Download All Entries", download_all_posts_path(@campaign.id), :class => 'btn' %>
      </div>
    </div>
    <div id="masonry-grid" class="row">
      <%= render partial: 'content_review_posts' %>
    </div>
    <div id="infinite-scrolling">
      <%= will_paginate(@post_collection) %>
    </div>
  </div>
</main>
